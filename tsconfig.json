{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "tsBuildInfoFile": "path/to/buildInfoFile.tsbuildinfo",
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "composite": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{
      "name": "next"
    }],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "libs/notion/notion.js", "libs/notion/notion.js"],
  "exclude": ["node_modules"]
}